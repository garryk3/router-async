  "repository": {
    "type": "git",
    "url": "https://github.com/router-async/router-async.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib",
    "watch": "yarn run clean && tsc -p . --watch",
    "transpile": "yarn run clean && tsc -p tsconfig.build.json",
    "test": "jest",
    "coverage": "jest --no-cache --coverage",
    "coveralls": "yarn coverage && cat ./coverage/remapped/lcov.info | coveralls"
  },
  "devDependencies": {
    "coveralls": "^2.11.15",
    "jest": "^18.1.0",
    "nyc": "^8.4.0",
    "ts-jest": "^18.0.3",
    "typescript": "^2.1.4"
  },
  "dependencies": {
    "path-to-regexp": "^1.6.0",
    "query-string": "^4.2.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*"
    ],
    "extension": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
